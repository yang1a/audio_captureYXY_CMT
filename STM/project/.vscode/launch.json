{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "DAPLink Debug",
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "pyocd",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/project.elf",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "none",
            "device": "STM32H743xx",
            "boardId": "",
            "cmsisPack": "",
            "svdFile": "",
            "preLaunchTask": "",
            "postDebugTask": ""
        },
        {
            "name": "DAPLink Attach",
            "type": "cortex-debug",
            "request": "attach",
            "servertype": "pyocd",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/Debug/project.elf",
            "device": "STM32H743xx",
            "showDevDebugOutput": "none"
        }
    ]
}
